{
    "name": "sanmai/cdek-sdk",
    "keywords": ["CDEK", "sdk", "delivery", "СДЭК", "служба доставки"],
    "homepage": "https://github.com/sanmai/cdek-sdk",
    "description": "СДЭК PHP SDK, библиотека для работы с API СДЭК",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Sokharev",
            "role": "Creator",
            "homepage": "https://github.com/greabock"
        },
        {
            "name": "JhaoDa",
            "role": "Developer",
            "homepage": "https://github.com/jhaoda"
        },
        {
            "name": "Alexey Kopytko",
            "role": "Developer",
            "email": "alexey@kopytko.com",
            "homepage": "https://www.alexeykopytko.com"
        }
    ],
    "support": {
        "chat": "https://t.me/phpcdeksdk",
        "issues": "https://github.com/sanmai/cdek-sdk/issues",
        "docs": "https://github.com/sanmai/cdek-sdk/blob/master/README.md"
    },
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer": "^1.11",
        "psr/log": "^1.0",
        "sanmai/pipeline": "^0.3.2|^2.0|^3.0.1"
    },
    "suggest": {
        "ext-ctype": "For flexible attribute name handling",
        "monolog/monolog": "For advanced logging and debugging",
        "sanmai/pindx": "For postal code validation",
        "hughgrigg/php-business-time": "Finds next working day, helps with delivery estimates"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "gamez/psr-testlogger": "^3.0",
        "illuminate/support": "^5.2",
        "infection/infection": ">=0.10",
        "laravel/framework": "^5.2",
        "phan/phan": "^1.1",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": ">=0.10",
        "phpunit/phpunit": "^6.5|^7.4",
        "vimeo/psalm": ">=1.1"
    },
    "autoload": {
        "psr-4": {
            "CdekSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\CdekSDK\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v0.6.x-dev"
        }
    }
}
