{
    "name": "appwilio/cdek-sdk",
    "keywords": ["CDEK", "sdk", "delivery"],
    "homepage": "https://github.com/appwilio/cdek-sdk",
    "description": "CDEK API SDK (cdek.ru)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Sokharev",
            "email": "greabock17@gmail.com",
            "role": "Creator"
        },
        {
            "name": "JhaoDa",
            "email": "jhaoda@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jhaoda@gmail.com",
        "issues": "https://github.com/appwilio/cdek-sdk/issues"
    },
    "require": {
        "php": ">=7.0",
        "jms/serializer": "^1.8",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*",
        "phpunit/phpunit": ">=6",
        "php-coveralls/php-coveralls": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.11",
        "codacy/coverage": "^1.4",
        "league/pipeline": "^1.0|^0.3",
        "phan/phan": "^0.12",
        "infection/infection": "~0.8",
        "phpstan/phpstan": "~0.9",
        "mockery/mockery": "^1.0",
        "vimeo/psalm": ">=1.1"
    },
    "autoload": {
        "psr-4": {
            "Appwilio\\CdekSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {"Tests\\Appwilio\\CdekSDK\\": "tests/"}
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
